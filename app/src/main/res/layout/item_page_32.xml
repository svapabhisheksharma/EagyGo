<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/transparent">

    <ImageView
        android:id="@+id/imgp32"
        android:layout_width="44dp"
        android:layout_height="44dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_remove_circle_24" />

    <TextView
        android:id="@+id/namep32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="6dp"
        android:textSize="14sp"
        android:textColor="#4f5962"
        android:lineSpacingExtra="2sp"
        android:text="Alok Jain"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/imgp32"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/datep32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="1dp"
        android:lineSpacingExtra="3sp"
        android:text="10-05-2020"
        android:textColor="#4f5962"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/imgp32"
        app:layout_constraintTop_toBottomOf="@+id/namep32" />

    <ImageView
        android:id="@+id/imageView50"
        android:layout_width="14dp"
        android:layout_height="14dp"
        android:layout_marginEnd="2dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView51"
        app:layout_constraintTop_toTopOf="@+id/imageView51"
        app:srcCompat="@drawable/ic_star_filled" />

    <ImageView
        android:id="@+id/imageView51"
        android:layout_width="14dp"
        android:layout_height="14dp"
        android:layout_marginEnd="2dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView52"
        app:layout_constraintTop_toTopOf="@+id/imageView52"
        app:srcCompat="@drawable/ic_star_filled" />

    <ImageView
        android:id="@+id/imageView52"
        android:layout_width="14dp"
        android:layout_height="14dp"
        android:layout_marginEnd="2dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView53"
        app:layout_constraintTop_toTopOf="@+id/imageView53"
        app:srcCompat="@drawable/ic_star_filled" />

    <ImageView
        android:id="@+id/imageView53"
        android:layout_width="14dp"
        android:layout_height="14dp"
        android:layout_marginEnd="2dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView54"
        app:layout_constraintTop_toTopOf="@+id/imageView54"
        app:srcCompat="@drawable/ic_star_filled" />

    <ImageView
        android:id="@+id/imageView54"
        android:layout_width="14dp"
        android:layout_height="14dp"
        android:layout_marginTop="14.6dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_star_filled" />

    <TextView
        android:id="@+id/reviewp32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:textSize="16sp"
        android:textColor="#4f5962"
        android:lineSpacingExtra="3sp"
        android:text="Nice Experience and ride!"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgp32" />

    <View
        android:id="@+id/divider8"
        android:layout_width="409dp"
        android:layout_height="1dp"
        android:layout_marginTop="23.5dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/reviewp32" />
</androidx.constraintlayout.widget.ConstraintLayout>